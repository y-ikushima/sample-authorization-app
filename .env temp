AWS_SERVICE_POSTGRES_PASSWORD=password
AWS_SERVICE_POSTGRES_DB=postgres

USER_SERVICE_POSTGRES_HOST=user_postgres
USER_SERVICE_POSTGRES_PORT=5432
USER_SERVICE_POSTGRES_USER=postgres
USER_SERVICE_POSTGRES_PASSWORD=password
USER_SERVICE_POSTGRES_DB=postgres

SYSTEM_SERVICE_POSTGRES_HOST=system_postgres
SYSTEM_SERVICE_POSTGRES_PORT=5432
SYSTEM_SERVICE_POSTGRES_USER=postgres
